{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "public/uploads/(.*)",
            "dest": "uploads/$1"
        },
        {
            "src": "/(.*)",
            "dest": "src/app.ts"
        }
    ]
}
